// Demonstration of conditionals - xor implementation
read AB {
    A := hd AB;
    B := tl AB;
    if A {
        if B {
            X := nil
        }
        else {
            X := cons nil nil
        }
    }
    else {
        if B {
            X := cons nil nil
        }
        else {
            X := nil
        }
    }
} write X
